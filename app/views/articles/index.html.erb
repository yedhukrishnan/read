<h1>Read</h1>

<div class="container-fluid">
  <div class="col-md-10 col-md-offset-1">
    <table class="table table-striped">
      <% @articles.each do |article| %>
      <tr>
        <td>
          <%= link_to article.title, article.url %>
        </td>
      </tr>
      <% end %>
    </table>
    <%= will_paginate @articles %>
  </div>
</div>
